{
  "name": "pskuza/rss-server",
  "type": "project",
  "description": "Simple rss server to setup your own feed.",
  "keywords": [
    "rss", "rss-generator", "feed-generator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Philip Skuza",
      "email": "philip.skuza+github@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "doctrine/cache": "1.7.1",
    "paragonie/easydb": "2.6.2",
    "nikic/fast-route": "1.3.0",
    "webmozart/assert": "1.3.0",
    "ext-apcu": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.2.6",
    "guzzlehttp/guzzle": "6.3.3",
    "codedungeon/phpunit-result-printer": "0.19.10",
    "sllh/composer-versions-check": "2.0.3"
  },
  "autoload": {
    "psr-4": {
      "pskuza\\rss\\": "src/"
    }
  },
  "archive": {
    "exclude": ["docker", "Dockerfile"]
  }
}
